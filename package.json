{
  "name": "helpdeskrequests",
  "productName": "Help Desk Requests",
  "version": "1.0.0",
  "description": "Project to create a help desk ticket system",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-windows":"electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\""
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.8.2",
    "electron-packager": "^8.7.2"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "electron-json-storage": "^3.0.6",
    "electron-pug": "^1.5.1",
    "express": "^4.15.3",
    "guid": "0.0.12",
    "pug": "^2.0.0-rc.2"
  }
}
